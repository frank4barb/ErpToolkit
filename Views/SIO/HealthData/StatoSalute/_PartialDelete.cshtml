@model ErpToolkit.Models.SIO.HealthData.StatoSalute
@{
    ViewData["Title"] = "StatoSalute Delete [" + (Model?.action ?? ' ') + "]";
    ViewData["TagHelper__READONLY_PAGE"] = "Y";
}
<div class="modal-dialog" role="document">
    @using (Html.BeginForm("Delete", "StatoSalute", FormMethod.Post))
    {
        @Html.AntiForgeryToken()
        <!--GESTISCE AZIONI CLICK PULSANTE-->
        @Html.Hidden("IsModalACTION", @ViewData["IsModalACTION"])
        @Html.Hidden("IsPageACTION", @ViewData["IsPageACTION"])
        @Html.Hidden("IsPageREDIRECT", @ViewData["IsPageREDIRECT"])
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="titolo">@ViewData["Title"]</h5>
                <button type="button" class="close" data-bs-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="container">
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                    Sei proprio sicuro di voler cancellare il seguente record?
                    <div class="form-group">
                        <label asp-for="SsClasse" class="control-label"></label>
                        <input asp-for="SsClasse" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsIdPaziente" class="control-label"></label>
                        <input asp-for="SsIdPaziente" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsIdEpisodio" class="control-label"></label>
                        <input asp-for="SsIdEpisodio" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsIdTipoDatoClinico" class="control-label"></label>
                        <input asp-for="SsIdTipoDatoClinico" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsValoreMinimo" class="control-label"></label>
                        <input asp-for="SsValoreMinimo" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsValoreMassimo" class="control-label"></label>
                        <input asp-for="SsValoreMassimo" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsValoreScelta" class="control-label"></label>
                        <input asp-for="SsValoreScelta" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsValoreTesto" class="control-label"></label>
                        <input asp-for="SsValoreTesto" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsNote" class="control-label"></label>
                        <input asp-for="SsNote" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsCodiceReferto" class="control-label"></label>
                        <input asp-for="SsCodiceReferto" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsDataAcquisizione" class="control-label"></label>
                        <input asp-for="SsDataAcquisizione" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsOraAcquisizione" class="control-label"></label>
                        <input asp-for="SsOraAcquisizione" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsStatoDatoClinico" class="control-label"></label>
                        <input asp-for="SsStatoDatoClinico" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsDataValidazione" class="control-label"></label>
                        <input asp-for="SsDataValidazione" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsOraValidazione" class="control-label"></label>
                        <input asp-for="SsOraValidazione" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="SsSequenza" class="control-label"></label>
                        <input asp-for="SsSequenza" class="form-control" />
                    </div>
                </div>
                <br />
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Annulla</button>
                <button type="button" class="btn btn-primary" onclick='updateModalWithContentForm(this, "@ViewData.TemplateInfo.HtmlFieldPrefix", "DeleteModale", @Html.Raw(System.Text.Json.JsonSerializer.Serialize(Model)))'>Elimina</button>
            </div>
        </div>
    }
</div>
