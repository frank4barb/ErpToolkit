                </div>
                <div class="form-group">
                    <label asp-for="PvCodiceReferto" class="control-label"></label>
                    <input asp-for="PvCodiceReferto" class="form-control" />
                    <span asp-validation-for="PvCodiceReferto" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="PvDataAcquisizione" class="control-label"></label>
                    <input asp-for="PvDataAcquisizione" class="form-control" />
                    <span asp-validation-for="PvDataAcquisizione" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="PvOraAcquisizione" class="control-label"></label>
                    <input asp-for="PvOraAcquisizione" class="form-control" />
                    <span asp-validation-for="PvOraAcquisizione" class="text-danger"></span>
