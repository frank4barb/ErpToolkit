                    <span asp-validation-for="PvValoreMinimo" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="PvValoreMassimo" class="control-label"></label>
                    <input asp-for="PvValoreMassimo" class="form-control" />
                    <span asp-validation-for="PvValoreMassimo" class="text-danger"></span>
