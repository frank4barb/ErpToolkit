                    <label asp-for="PvOraValidazione" class="control-label"></label>
                    <input asp-for="PvOraValidazione" class="form-control" />
                    <span asp-validation-for="PvOraValidazione" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="PvSequenza" class="control-label"></label>
                    <input asp-for="PvSequenza" class="form-control" />
                    <span asp-validation-for="PvSequenza" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <p></p><input type="submit" value="Ricarica" class="btn btn-primary" />
