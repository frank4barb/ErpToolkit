                <div class="form-group">
                    <label asp-for="PvValoreScelta" class="control-label"></label>
                    <input asp-for="PvValoreScelta" class="form-control" />
                    <span asp-validation-for="PvValoreScelta" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="PvValoreTesto" class="control-label"></label>
