@model ErpToolkit.Models.SIO.HealthData.SelParametroVitale
@{
    ViewData["Title"] = "ParametroVitale Select";
}
<div class="row justify-content-start align-items-end">
    @using (Html.BeginForm("Index", "ParametroVitale", FormMethod.Post))
