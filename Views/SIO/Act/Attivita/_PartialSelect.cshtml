                    <label asp-for="SelAvAttributi2" class="control-label"></label>
                    <input asp-for="SelAvAttributi2" class="form-control" />
                    <span asp-validation-for="SelAvAttributi2" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <p></p><input type="submit" value="Ricarica" class="btn btn-primary" />
                </div>
            </div>
        </div>
