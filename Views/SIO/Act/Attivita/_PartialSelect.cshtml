                    <span asp-validation-for="SelAvDescrizione" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="SelAvNote" class="control-label"></label>
                    <input asp-for="SelAvNote" class="form-control" />
                    <span asp-validation-for="SelAvNote" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="SelAvFiltroRegimeErogazione" class="control-label"></label>
