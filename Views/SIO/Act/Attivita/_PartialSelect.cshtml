                </div>
                <div class="form-group">
                    <label asp-for="SelAvInEvidenza" class="control-label"></label>
                    <input asp-for="SelAvInEvidenza" class="form-control" />
                    <span asp-validation-for="SelAvInEvidenza" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="SelAvIdTipoAttivita" class="control-label"></label>
                    <input asp-for="SelAvIdTipoAttivita" class="form-control" />
                    <span asp-validation-for="SelAvIdTipoAttivita" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="SelAvRoutine" class="control-label"></label>
                    <input asp-for="SelAvRoutine" class="form-control" />
                    <span asp-validation-for="SelAvRoutine" class="text-danger"></span>
