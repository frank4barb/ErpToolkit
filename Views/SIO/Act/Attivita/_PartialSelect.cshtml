                    <span asp-validation-for="SelAvCostoMedio" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="SelAvIdGruppo" class="control-label"></label>
                    <input asp-for="SelAvIdGruppo" class="form-control" />
