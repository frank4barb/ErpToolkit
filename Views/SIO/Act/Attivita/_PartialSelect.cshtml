                </div>
                <div class="form-group">
                    <label asp-for="SelAvAttivitaPreferenziale" class="control-label"></label>
                    <input asp-for="SelAvAttivitaPreferenziale" class="form-control" />
                    <span asp-validation-for="SelAvAttivitaPreferenziale" class="text-danger"></span>
                </div>
